        <TableLayout android:layout_width="fill_parent"
            android:layout_height="fill_parent">
			<TableRow
			    android:layout_width="wrap_content"
			    android:layout_height="wrap_content"
			    android:orientation="horizontal">
        
			<TextView 
			    android:id="@+id/textT"
			    android:layout_width="wrap_content"
